			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "integratedTerminal",
			"sourceMaps": true,
			"outFiles": []
		},
		{
			"name": "Test",
			"type": "node",
