{
  "name": "ember-analyzer",
  "version": "0.0.1",
  "description": "Utils for analyzing ember code",
  "main": "bin/server.js",
  "scripts": {
    "dev-setup": "npm install && typings install && npm link",
    "start": "bin/server.js",
    "test": "node_modules/.bin/mocha ./dist/test/index.js",
    "typescript-version": "tsc -v",
    "watch": "tsc -w -p .",
    "build": "tsc -p .",
    "test-watch": "node_modules/.bin/mocha --watch ./dist/test/index.js"
  },
  "bin": {
    "ember-analyzer-start-server": "bin/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^5.8.38",
    "htmlbars": "^0.14.16",
    "koa": "^2.0.0",
    "koa-router": "^7.0.1",
    "lodash": "^4.8.2",
    "progress": "^1.1.8",
    "recast": "^0.11.3",
    "walk-sync": "^0.2.6"
  },
  "devDependencies": {
    "mocha": "^2.4.5",
    "testdouble": "^1.4.1",
    "typescript": "1.8.10",
    "typings": "1.0.4"
  }
}
